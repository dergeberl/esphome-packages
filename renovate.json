{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "regexManagers": [
    {
      "fileMatch": ["^workflows/example-build.yaml$"],
      "matchStrings": [
        "version: \"(?<currentValue>.*)\" # \"(?<depName>.*?)\"\\s*"
      ],
      "datasourceTemplate": "docker"
    }
  ]
}
